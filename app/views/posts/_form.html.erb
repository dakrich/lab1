<%= form_for @post do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_field :title %> <br>
    <br>
    <%= f.label :author %> <br>
    <%= f.text_field :author %> <br>
    <br>
    <%= f.label :genre %> <br>
    <%= f.text_field :genre %> <br>
    <br>
    <%= f.label :price %> <br>
    <%= f.text_field :price %> <br>
    <br>
    <%= f.label :publisheddate %> <br>
    <%= f.text_field :publisheddate %> <br>
    <br>
    <%= f.submit %>
<%end%>